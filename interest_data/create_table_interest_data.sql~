CREATE TABLE interest_rates_time_bankofengland (
    id SERIAL,
    date_of_change date,
    interest_rate  DECIMAL(2,2),
    PRIMARY KEY (id)
);

CREATE TABLE interest_rates_time_federalreserve (
    id SERIAL,
    date_of_change date,
    interest_rate  DECIMAL(2,2),
    PRIMARY KEY (id)
);

CREATE TABLE interest_rates_time_europeancentral (
    id SERIAL,
    date_of_change date,
    interest_rate  DECIMAL(2,2),
    PRIMARY KEY (id)
);
